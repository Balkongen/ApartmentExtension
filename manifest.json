{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension example.",
    "permissions": [
        "activeTab",
        "scripting"              
    ],
    "action": {
        "default_popup": "popup/popup.html"
    
    },
    "background": {
        "service_worker": "background.js"    
    },
    "content_scripts": [
        {
        "matches": ["https://hemnet.se/*"],        
        "js": ["contentScript.js"],         
        "run_at": "document_idle"            
        }
    ]
    
}
  